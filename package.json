{
	"name": "isitveganphoto",
	"version": "0.0.5",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"ios": "npm run build && npx cap sync && npx cap update ios && npx cap copy ios && npx cap run ios",
		"ios-reload": "npm run build && npx cap sync && npx cap update ios && npx cap copy ios && cap run ios -l --external --port=5173"
	},
	"devDependencies": {
		"@capacitor/cli": "^6.1.2",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^0.21.16",
		"clsx": "^2.1.1",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwind-merge": "^2.5.4",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "^3.4.9",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"dependencies": {
		"@capacitor/android": "^6.1.2",
		"@capacitor/camera": "^5.0.10",
